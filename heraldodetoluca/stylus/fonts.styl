@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?bhsscg');
	src:url('../fonts/icomoon.eot?#iefixbhsscg') format('embedded-opentype'),
		url('../fonts/icomoon.woff?bhsscg') format('woff'),
		url('../fonts/icomoon.ttf?bhsscg') format('truetype'),
		url('../fonts/icomoon.svg?bhsscg#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-menu:before {
	content: "\e600";
}
.icon-facebook:before {
	content: "\e601";
}
.icon-twitter:before {
	content: "\e602";
}
.icon-youtube:before {
	content: "\e603";
}
.icon-flickr:before {
	content: "\e604";
}
.icon-arrow-left:before {
	content: "\e605";
}
.icon-arrow-right:before {
	content: "\e606";
}
